@isTest
private class XmlToJsonTest {

	static String testXMLClosedElement = '<e/>';
	static String testXMLElement = '<e>text</e>';
	static String testXMLClosedElementAttr = '<e name="val"/>';
	static String testXMLElementAttr = '<e name="val">text<k kname="kval"></k></e>';
	static String testXMLComplexUniqueElement = '<e><a>textA</a><b>textB</b></e>';
	static String testXMLComplexSameElement = '<e><a>text1</a><a>text2</a></e>';
	static String testXMLComplexMixed = '<e>textE<a>textA</a></e>';
	static String testXMLNamespace = '<ns1:e xmlns:ns1="http://www.none.com/ns1"><nsp:a xmlns:nsp="http://www.none.com/nsp">text1</nsp:a><a>text2</a></ns1:e>';	

	@isTest static void testClosedElement() {
		String retJson = XmlToJson.convertXMLToJson(testXMLClosedElement);
		System.debug(retJson);
		System.assert(retJson.equals('{"e":{"#text":""}}'));
	}

	@isTest static void testElementWText() {
		String retJson = XmlToJson.convertXMLToJson(testXMLElement);
		System.debug(retJson);
		System.assert(retJson.equals('{"e":{"#text":"text"}}'));
	}

	@isTest static void testComplexUniqueElement() {
		String retJson = XmlToJson.convertXMLToJson(testXMLComplexUniqueElement);
		System.debug(retJson);
		System.assert(retJson.equals('{"e":{"b":{"#text":"textB"},"a":{"#text":"textA"},"#text":""}}'));
	}

	@isTest static void testComplexSameElement() {
		String retJson = XmlToJson.convertXMLToJson(testXMLComplexSameElement);
		System.debug(retJson);
		System.assert(retJson.equals('{"e":{"a":[{"#text":"text1"},{"#text":"text2"}],"#text":""}}'));
	}

	@isTest static void testComplexMixed() {
		String retJson = XmlToJson.convertXMLToJson(testXMLComplexMixed);
		System.debug(retJson);
		System.assert(retJson.equals('{"e":{"a":{"#text":"textA"},"#text":"textE"}}'));
	}

	@isTest static void testAttrElement() {
		String retJson = XmlToJson.convertXMLToJson(testXMLElementAttr);
		System.debug(retJson);
		System.assert(retJson.equals(
				'{"e":{"k":{"@kname":"kval","#text":""},"@name":"val","#text":"text"}}'));
	}

	@isTest static void testClosedAttrElement() {
		String retJson = XmlToJson.convertXMLToJson(testXMLClosedElementAttr);
		System.debug(retJson);
		System.assert(retJson.equals('{"e":{"@name":"val","#text":""}}'));
	}

	@isTest static void testNamespace() {
		String retJson = XmlToJson.convertXMLToJson(testXMLNamespace);
		System.debug(retJson);
		System.assert(retJson.equals('{"e":{"a":[{"#ns":"http://www.none.com/nsp","#text":"text1"},{"#text":"text2"}],"#ns":"http://www.none.com/ns1","#text":""}}'));
	}


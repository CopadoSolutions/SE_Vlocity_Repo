    <fields>
        <fullName>Product_Family_Image__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Product_Image__c), 
IF(ISPICKVAL(Product2.Family, &apos;Bundle&apos;),&apos;/resource/VDOProductResources/images/TNBundle.png&apos;,&apos;&apos;)+ 
IF(ISPICKVAL(Product2.Family, &apos;Mobile&apos;),
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Watch&apos;), &apos;/resource/VDOProductResources/images/TNWearable.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Earphones&apos;), &apos;/resource/VDOProductResources/images/TNHeadphones.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Tariff&apos;), &apos;/resource/VDOProductResources/images/TNTariff.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Grant&apos;), &apos;/resource/VDOProductResources/images/TNTariff.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Extra&apos;), &apos;/resource/VDOProductResources/images/TNStar.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Service&apos;), &apos;/resource/VDOProductResources/images/TNStar.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Discount&apos;), &apos;/resource/VDOProductResources/images/TNDiscount.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Offer&apos;), &apos;/resource/VDOProductResources/images/TNStar.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Technology&apos;), &apos;/resource/VDOProductResources/images/TNSim.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__Type__c, &apos;Bundle&apos;), &apos;/resource/VDOProductResources/images/TNBundle.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Handset&apos;), &apos;/resource/VDOProductResources/images/TNMobile.png&apos;,&apos;&apos;),
&apos;&apos;)+
IF(ISPICKVAL(Product2.Family, &apos;Landline&apos;),
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Extra&apos;), &apos;/resource/VDOProductResources/images/TNPhone.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Service&apos;), &apos;/resource/VDOProductResources/images/TNPBX.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Offer&apos;), &apos;/resource/VDOProductResources/images/TNPhone.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Installation&apos;), &apos;/resource/VDOProductResources/images/TNInstall.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Software&apos;), &apos;/resource/VDOProductResources/images/TNPBX.png&apos;,&apos;&apos;),
&apos;&apos;)+
IF(ISPICKVAL(Product2.Family, &apos;Internet&apos;),
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Product&apos;), &apos;/resource/VDOProductResources/images/TNInternet.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Service&apos;), &apos;/resource/VDOProductResources/images/TNLAN.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Technology&apos;), &apos;/resource/VDOProductResources/images/TNPBX.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Installation&apos;), &apos;/resource/VDOProductResources/images/TNInstall.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Software&apos;), &apos;/resource/VDOProductResources/images/TNInternet.png&apos;,&apos;&apos;), 
&apos;&apos;)+
IF(ISPICKVAL(Product2.Family, &apos;Support&apos;),&apos;/resource/VDOProductResources/images/TNSupport.png&apos;,&apos;&apos;)+ 
IF(ISPICKVAL(Product2.Family, &apos;Discount&apos;),&apos;/resource/VDOProductResources/images/TNDiscount.png&apos;,&apos;&apos;)+ 
IF(ISPICKVAL(Product2.Family, &apos;TV&apos;),
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Extra&apos;), &apos;/resource/VDOProductResources/images/TNTV.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Service&apos;), &apos;/resource/VDOProductResources/images/TNTV.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Package&apos;), &apos;/resource/VDOProductResources/images/TNTV.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__SubType__c, &apos;Technology&apos;), &apos;/resource/VDOProductResources/images/TNTV.png&apos;,&apos;&apos;)+ 
          IF(ISPICKVAL(Product2.vlocity_cmt__Type__c, &apos;Bundle&apos;), &apos;/resource/VDOProductResources/images/TNBundle.png&apos;,&apos;&apos;),
&apos;&apos;),
Product_Image__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
